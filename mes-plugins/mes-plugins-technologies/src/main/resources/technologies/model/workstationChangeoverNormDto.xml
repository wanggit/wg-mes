<model name="workstationChangeoverNormDto" activable="true"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schema.qcadoo.org/model"
       xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">

    <fields>
        <string name="name" unique="true">
            <validatesLength max="1024"/>
        </string>
        <string name="description">
            <validatesLength max="2048"/>
        </string>
        <integer name="workstationType_id"/>
        <string name="workstationTypeNumber"/>
        <integer name="workstation_id"/>
        <string name="workstationNumber"/>
        <enum name="changeoverType" values="01anyChange,02betweenValues"
              default="01anyChange" required="true"/>
        <integer name="attribute_id"/>
        <string name="attributeNumber"/>
        <string name="fromAttributeValueValue"/>
        <string name="toAttributeValueValue"/>
        <integer name="duration" required="true"/>
        <boolean name="isParallel"/>
    </fields>

    <hooks/>

    <masterModel plugin="technologies" model="workstationChangeoverNorm"/>
</model>